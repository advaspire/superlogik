$("#adv-leads-form").on("submit", function (e) {
  if (!e.isDefaultPrevented()) {
    const name = $("#adv-name").val(),
      email = $("#adv-email").val(),
      phone = $("#adv-phone").val();

    if (!(name || email || phone)) {
      UIkit.modal($("#adv-lead-error-modal")).show();
      return;
    }

    $.post(API_URL + "/web-leads", {
      name,
      email,
      phone,
    })
      .done(function (data) {
        UIkit.modal($("#adv-lead-success-modal")).show();
        $("#adv-leads-form")[0].reset();
      })
      .fail(function (xhr, status, error) {
        UIkit.modal($("#adv-lead-error-modal")).show();
      });
  }
  return false;
});
